{
  "expo": {
    "name": "Noted",
    "slug": "noted-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "notedrn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.liight.notedrn"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/noted-icon-foreground.png",
        "backgroundImage": "./assets/images/noted-icon-background.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.liight.notedrn"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/noted-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/noted-logo-dark.png"
          }
        }
      ],
      [
        "expo-font",
        {
          "android": {
            "fonts": [
              {
                "fontFamily": "Inter",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Inter_18pt-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/fonts/Inter_18pt-Bold.ttf",
                    "weight": 700
                  }
                ]
              },
              {
                "fontFamily": "Poppins",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Poppins-Regular.ttf",
                    "weight": 400
                  }
                ]
              }
            ]
          }
        }
      ],
      "expo-sqlite",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
